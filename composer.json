{
  "name": "shibuyakosuke/laravel-ddl-export",
  "description": "Export Table definition to spreadsheet by artisan command.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "ShibuyaKosuke",
      "email": "shibuya@imagenious.co.jp"
    }
  ],
  "require": {
    "php": "^7.4 | ^8.0",
    "laravel/framework": "^6.0 | ^7.24 | ^8.0",
    "maatwebsite/excel": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "ShibuyaKosuke\\LaravelDdlExport\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ShibuyaKosuke\\LaravelDdlExport\\Providers\\ServiceProvider"
      ],
      "aliases": {
        "Table": "ShibuyaKosuke\\LaravelDdlExport\\Facades\\Table",
        "Column": "ShibuyaKosuke\\LaravelDdlExport\\Facades\\Column"
      }
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^0.12.85"
  }
}
